<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.common.mailtemplatemanage.repository.MailTemplateRepository">

    <select id="findMailTemplateByName" parameterType="Map" resultType="DataModel">
        SELECT * FROM comm_mail_template WHERE template_name = #{templateName}
    </select>

</mapper>
